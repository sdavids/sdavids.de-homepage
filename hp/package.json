{
  "name": "sdavids.de-homepage",
  "description": "sdavids.de-homepage",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "contributors": [
    "Sebastian Davids <sdavids@gmx.de>"
  ],
  "homepage": "https://github.com/sdavids/sdavids.de-homepage",
  "bugs": "https://github.com/sdavids/sdavids.de-homepage/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sdavids/sdavids.de-homepage.git"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "scripts/prepare.sh",
    "build": "scripts/build.sh",
    "clean": "rm -rf dist src/s/app.css",
    "clean:node": "rm -rf node_modules package-lock.json",
    "compress:files": "scripts/compress-files.sh dist",
    "create:google-verification-file": "scripts/create-google-verification-file.sh dist",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "minify:html": "scripts/minify-html.sh",
    "prettier": "npx prettier --write '.'",
    "start": "browser-sync src -f src -b \"google chrome\" --no-notify",
    "start:tailwind": "npx tailwindcss --config tailwind.config.cjs -i src/s/app.src.css -o src/s/app.css --watch",
    "tailwind:build": "npx tailwindcss --config tailwind.config.cjs -i src/s/app.src.css -o src/s/app.css --minify"
  },
  "devDependencies": {
    "@rvxlab/tailwind-plugin-ios-full-height": "1.1.0",
    "browser-sync": "2.27.7",
    "browserslist": "4.19.1",
    "esbuild": "0.14.12",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-json": "3.1.0",
    "html-minifier": "4.0.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "lint-staged": "12.3.2",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.17"
  }
}
