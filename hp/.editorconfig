[*.html]
max_line_length = 80

[*.css]
max_line_length = 80

[*.{js,mjs}]
max_line_length = 80

[*.{json,webmanifest}]
max_line_length = 80

[*.{svg,xml}]
max_line_length = 80

[*.{age,gpg,pub}]
max_line_length = off
insert_final_newline = false
trim_trailing_whitespace = false
